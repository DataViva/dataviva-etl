	
if '$(executarExtracao)'='S' then


	//Dados Originais
	SECEX:
	LOAD      
	@1:4 as ANO,
	@5:6 as MES,
	@7:14 as NCM,
	@15:17 as PAIS,
	@18:19 as UF,
	@20:23 as PORTO,
	@24:30 as MUNICÍPIO,
	@31:32 as UNIDADE,
	@33:47 as QUANTIDADE,
	@48:62 as KGLIQUIDO,
	@63:n as VALORFOB
	
	FROM
	[F:\Banco de dados\MDIC\Dados originais\EPEMG_BREX20*.txt]
	(fix, codepage is 1252);
	
	Concatenate
	
	LOAD CO_ANO as ANO, 
		     CO_MES as MES, 
		     CO_NCM as NCM, 
		     CO_PAIS  as PAIS,
		     CO_UF as UF, 
		     CO_MUN_GEO as MUNICÍPIO, 
		     CO_PORTO as PORTO, 
		     CO_UNID as UNIDADE, 
		     [SUM(MES.QT_ESTAT)]  as QUANTIDADE, 
		     [SUM(MES.KG_LIQUIDO)] as KGLIQUIDO, 
		     [SUM(MES.VL_FOB)] as VALORFOB
	
	FROM
	[F:\Banco de dados\MDIC\Dados originais\Exportacao_2012.txt]
	(txt, codepage is 1252, embedded labels, delimiter is '|', msq);
	
	
	
	
	
	
	
	
	
	
	
	
	store SECEX into 
	F:\Banco de dados\MDIC\Dados originais\EPEMG_BREX20.qvd (qvd);
	
	drop table SECEX;
	
	
	
	
	//Dados Enviados
	
	SECEX:
	LOAD 	
		Year as ANO, 
		Municipality_ID as MunIDMDIC,
		TransactedProduct_ID_NCM as NCM2007,	
		[TransactionAmount_US$_FOB] as VALORFOB
     
	FROM
	[F:\Product Space\Dados Enviados para GV\2012_07_09_SECEX\MDIC_*.txt]
	(txt, utf8, embedded labels, delimiter is '|', msq);


	store SECEX into 
	F:\Product Space\Dados Enviados para GV\2012_07_09_SECEX\MDIC_All.qvd (qvd);

	drop table SECEX;
	
	
	
	
	
end if	

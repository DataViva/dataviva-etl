/**
* ANO 2001
*/
/**
  VALIDAÇÃO PRESENCIAL
*/
PRESENCIAL_2001:
load 
  VAGAS AS PRESENCIAL_2001.VAGAS,
  CONCLUINTES AS PRESENCIAL_2001.CONCLUINTES,
  CONCLUINTES_2000 AS PRESENCIAL_2001.CONCLUINTES_2000,
  INGRESSANTES AS PRESENCIAL_2001.INGRESSANTES,
  MATRICULADOS AS PRESENCIAL_2001.MATRICULADOS,
  ANO AS PRESENCIAL_2001.ANO
FROM [qvd\2001\GRADUACAO_PRESENCIAL_T.qvd](qvd);

VALIDA_PRESENCIAL_2001:
load
SUM(PRESENCIAL_2001.VAGAS) AS PRESENCIAL_2001.TOTAL_VAGAS,
SUM(PRESENCIAL_2001.CONCLUINTES) AS PRESENCIAL_2001.TOTAL_CONCLUINTES,
SUM(PRESENCIAL_2001.CONCLUINTES_2000) AS PRESENCIAL_2001.TOTAL_CONCLUINTES_2000,
SUM(PRESENCIAL_2001.INGRESSANTES) AS PRESENCIAL_2001.TOTAL_INGRESSANTES,
SUM(PRESENCIAL_2001.MATRICULADOS) as PRESENCIAL_2001.TOTAL_MATRICULADOS
Resident PRESENCIAL_2001 Group by PRESENCIAL_2001.ANO;


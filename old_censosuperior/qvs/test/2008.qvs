/**
  VALIDAÇÃO PRESENCIAL
*/
PRESENCIAL_2008:
load 
  VAGAS AS PRESENCIAL_2008.VAGAS,
  CONCLUINTES AS PRESENCIAL_2008.CONCLUINTES,
  INGRESSANTES AS PRESENCIAL_2008.INGRESSANTES,
  MATRICULADOS AS PRESENCIAL_2008.MATRICULADOS,
  ANO AS PRESENCIAL_2008.ANO
FROM [qvd\2008\GRADUACAO_PRESENCIAL_T.qvd](qvd);

VALIDA_PRESENCIAL_2008:
load
SUM(PRESENCIAL_2008.VAGAS) AS PRESENCIAL_2008.TOTAL_VAGAS,
SUM(PRESENCIAL_2008.CONCLUINTES) AS PRESENCIAL_2008.TOTAL_CONCLUINTES,
SUM(PRESENCIAL_2008.INGRESSANTES) AS PRESENCIAL_2008.TOTAL_INGRESSANTES,
SUM(PRESENCIAL_2008.MATRICULADOS) as PRESENCIAL_2008.TOTAL_MATRICULADOS
Resident PRESENCIAL_2008 Group by PRESENCIAL_2008.ANO;




for anoValida = 2009 to 2012
  NoConcatenate
  load_table:
  load 
    VAGAS AS load_table.VAGAS,
    CONCLUINTES AS load_table.CONCLUINTES,
    INGRESSANTES AS load_table.INGRESSANTES,
    MATRICULADOS AS load_table.MATRICULADOS,
    ANO AS load_table.ANO
  FROM [qvd\$(anoValida)\CURSO_T.qvd](qvd);

  [VALIDACAO_$(anoValida)]:
  load
  SUM(VALIDACAO.VAGAS) AS VALIDACAO.TOTAL_VAGAS,
  SUM(VALIDACAO.CONCLUINTES) AS VALIDACAO.TOTAL_CONCLUINTES,
  SUM(VALIDACAO.INGRESSANTES) AS VALIDACAO.TOTAL_INGRESSANTES,
  SUM(VALIDACAO.MATRICULADOS) as VALIDACAO.TOTAL_MATRICULADOS
  Resident load_table Group by VALIDACAO.ANO;
Next anoValida;
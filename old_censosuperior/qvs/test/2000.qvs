/**
* ANO 2000
*/

/**
	VALIDAÇÃO PRESENCIAL
*/
PRESENCIAL:
load 
	VAGAS AS PRESENCIAL.VAGAS,
	CONCLUINTES AS PRESENCIAL.CONCLUINTES,
	INGRESSANTES AS PRESENCIAL.INGRESSANTES,
	MATRICULADOS AS PRESENCIAL.MATRICULADOS,
	ANO AS PRESENCIAL.ANO
FROM [qvd\2000\GRADUACAO_PRESENCIAL_T.qvd](qvd);

VALIDA_PRESENCIAL:
load
SUM(PRESENCIAL.VAGAS) AS PRESENCIAL.TOTAL_VAGAS,
SUM(PRESENCIAL.CONCLUINTES) AS PRESENCIAL.TOTAL_CONCLUINTES,
SUM(PRESENCIAL.INGRESSANTES) AS PRESENCIAL.TOTAL_INGRESSANTES,
SUM(PRESENCIAL.MATRICULADOS) as PRESENCIAL.TOTAL_MATRICULADOS
Resident PRESENCIAL Group by PRESENCIAL.ANO;



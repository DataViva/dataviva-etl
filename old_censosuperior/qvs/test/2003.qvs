/**
* ANO 2003
*/
/**
  VALIDAÇÃO PRESENCIAL
*/
PRESENCIAL_2003:
load 
  VAGAS AS PRESENCIAL_2003.VAGAS,
  CONCLUINTES AS PRESENCIAL_2003.CONCLUINTES,
  INGRESSANTES AS PRESENCIAL_2003.INGRESSANTES,
  MATRICULADOS AS PRESENCIAL_2003.MATRICULADOS,
  ANO AS PRESENCIAL_2003.ANO
FROM [qvd\2003\GRADUACAO_PRESENCIAL_T.qvd](qvd);

VALIDA_PRESENCIAL_2003:
load
SUM(PRESENCIAL_2003.VAGAS) AS PRESENCIAL_2003.TOTAL_VAGAS,
SUM(PRESENCIAL_2003.CONCLUINTES) AS PRESENCIAL_2003.TOTAL_CONCLUINTES,
SUM(PRESENCIAL_2003.INGRESSANTES) AS PRESENCIAL_2003.TOTAL_INGRESSANTES,
SUM(PRESENCIAL_2003.MATRICULADOS) as PRESENCIAL_2003.TOTAL_MATRICULADOS
Resident PRESENCIAL_2003 Group by PRESENCIAL_2003.ANO;


/**
  VALIDAÇÃO PRESENCIAL
*/
PRESENCIAL_2007:
load 
  VAGAS AS PRESENCIAL_2007.VAGAS,
  CONCLUINTES AS PRESENCIAL_2007.CONCLUINTES,
  INGRESSANTES AS PRESENCIAL_2007.INGRESSANTES,
  MATRICULADOS AS PRESENCIAL_2007.MATRICULADOS,
  ANO AS PRESENCIAL_2007.ANO
FROM [qvd\2007\GRADUACAO_PRESENCIAL_T.qvd](qvd);

VALIDA_PRESENCIAL_2007:
load
SUM(PRESENCIAL_2007.VAGAS) AS PRESENCIAL_2007.TOTAL_VAGAS,
SUM(PRESENCIAL_2007.CONCLUINTES) AS PRESENCIAL_2007.TOTAL_CONCLUINTES,
SUM(PRESENCIAL_2007.INGRESSANTES) AS PRESENCIAL_2007.TOTAL_INGRESSANTES,
SUM(PRESENCIAL_2007.MATRICULADOS) as PRESENCIAL_2007.TOTAL_MATRICULADOS
Resident PRESENCIAL_2007 Group by PRESENCIAL_2007.ANO;



INSERT INTO stat_ybu (year, bra_id, university_id, enrollments, entrants, graduates, top_course_enrollments, course_enrollments, top_course_entrants, course_entrants, top_course_graduates, course_graduates)

select sum(stat_val) from dataviva.attrs_ybs where year = '2010' and stat_id = 'gdp' and length(bra_id) = 1;
select avg(stat_val) from dataviva.attrs_ybs where year = '2010' and stat_id = 'life_exp' and length(bra_id) = 3;
select sum(stat_val) from dataviva.attrs_ybs where year = '2010' and stat_id = 'pop' and length(bra_id) = 3;
select avg(stat_val) from dataviva.attrs_ybs where year = '2010' and stat_id = 'gdp_pc' and length(bra_id) = 1;
select avg(stat_val) from dataviva.attrs_ybs where year = '2010' and stat_id = 'hdi' and length(bra_id) = 3;
select avg(eci) from dataviva.secex_ymb where year = '2014' and eci is not null and length(bra_id) = 3;

select name_pt, export_val from dataviva.secex_ymbp a INNER JOIN dataviva.attrs_hs b ON a.hs_id = b.id where year = '2014' and bra_id = '4sp' and length(hs_id) = 6 order by export_val desc limit 1;
select name_pt, import_val from dataviva.secex_ymbp a INNER JOIN dataviva.attrs_hs b ON a.hs_id = b.id where year = '2014' and bra_id = '4sp' and length(hs_id) = 6 order by import_val desc limit 1;
select export_val from dataviva.secex_ymb where year = '2014' and bra_id = '4sp' and month = 0;
select import_val from dataviva.secex_ymb where year = '2014' and bra_id = '4sp' and month = 0;
select wld_id, export_val from dataviva.secex_ymbw where year = '2014'  and bra_id = '4sp' and length(wld_id) = 5 order by export_val desc limit 1;
select wld_id, import_val from dataviva.secex_ymbw where year = '2014'  and bra_id = '4sp' and length(wld_id) = 5 order by import_val desc limit 1;
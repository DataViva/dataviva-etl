/**
  VALIDAÇÃO PRESENCIAL
*/
PRESENCIAL_2004:
load 
  VAGAS AS PRESENCIAL_2004.VAGAS,
  CONCLUINTES AS PRESENCIAL_2004.CONCLUINTES,
  INGRESSANTES AS PRESENCIAL_2004.INGRESSANTES,
  MATRICULADOS AS PRESENCIAL_2004.MATRICULADOS,
  ANO AS PRESENCIAL_2004.ANO
FROM [qvd\2004\GRADUACAO_PRESENCIAL_T.qvd](qvd);

VALIDA_PRESENCIAL_2004:
load
SUM(PRESENCIAL_2004.VAGAS) AS PRESENCIAL_2004.TOTAL_VAGAS,
SUM(PRESENCIAL_2004.CONCLUINTES) AS PRESENCIAL_2004.TOTAL_CONCLUINTES,
SUM(PRESENCIAL_2004.INGRESSANTES) AS PRESENCIAL_2004.TOTAL_INGRESSANTES,
SUM(PRESENCIAL_2004.MATRICULADOS) as PRESENCIAL_2004.TOTAL_MATRICULADOS
Resident PRESENCIAL_2004 Group by PRESENCIAL_2004.ANO;


SET PRESENCIAL_2004_TOTAL_VAGAS=2320421;

load
SUM(PRESENCIAL_2005.VAGAS) AS PRESENCIAL_2005.TOTAL_VAGAS,
SUM(PRESENCIAL_2005.CONCLUINTES) AS PRESENCIAL_2005.TOTAL_CONCLUINTES,
SUM(PRESENCIAL_2005.INGRESSANTES) AS PRESENCIAL_2005.TOTAL_INGRESSANTES,
SUM(PRESENCIAL_2005.MATRICULADOS) as PRESENCIAL_2005.TOTAL_MATRICULADOS
Resident PRESENCIAL_2005 Group by PRESENCIAL_2005.ANO;



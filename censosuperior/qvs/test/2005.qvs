/**
  VALIDAÇÃO PRESENCIAL
*/
PRESENCIAL_2005:
load 
  VAGAS AS PRESENCIAL_2005.VAGAS,
  CONCLUINTES AS PRESENCIAL_2005.CONCLUINTES,
  INGRESSANTES AS PRESENCIAL_2005.INGRESSANTES,
  MATRICULADOS AS PRESENCIAL_2005.MATRICULADOS,
  ANO AS PRESENCIAL_2005.ANO
FROM [qvd\2005\GRADUACAO_PRESENCIAL_T.qvd](qvd);


/**
* ANO 2002
*/
/**
  VALIDAÇÃO PRESENCIAL
*/
PRESENCIAL_2002:
load 
  VAGAS AS PRESENCIAL_2002.VAGAS,
  CONCLUINTES AS PRESENCIAL_2002.CONCLUINTES,
  INGRESSANTES AS PRESENCIAL_2002.INGRESSANTES,
  MATRICULADOS AS PRESENCIAL_2002.MATRICULADOS,
  ANO AS PRESENCIAL_2002.ANO
FROM [qvd\2002\GRADUACAO_PRESENCIAL_T.qvd](qvd);

VALIDA_PRESENCIAL_2002:
load
SUM(PRESENCIAL_2002.VAGAS) AS PRESENCIAL_2002.TOTAL_VAGAS,
SUM(PRESENCIAL_2002.CONCLUINTES) AS PRESENCIAL_2002.TOTAL_CONCLUINTES,
SUM(PRESENCIAL_2002.INGRESSANTES) AS PRESENCIAL_2002.TOTAL_INGRESSANTES,
SUM(PRESENCIAL_2002.MATRICULADOS) as PRESENCIAL_2002.TOTAL_MATRICULADOS
Resident PRESENCIAL_2002 Group by PRESENCIAL_2002.ANO;

